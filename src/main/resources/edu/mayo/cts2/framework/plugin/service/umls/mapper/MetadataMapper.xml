<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.mayo.cts2.framework.plugin.service.umls.mapper.MetadataMapper">
  
  <select id="getReleaseInfoDTO" 
  	resultType="edu.mayo.cts2.framework.plugin.service.umls.mapper.ReleaseInfoDTO">
		select 
			name,
			release_date as releaseDate,
			description
		from
			release_info  	
  </select>

</mapper>